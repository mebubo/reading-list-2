{
    "manifest_version": 3,
    "name": "Reading List Manager",
    "version": "1.0",
    "permissions": ["tabs", "storage"],
    "action": {
      "default_popup": "popup.html",
      "default_title": "Manage Reading List"
    },
    "background": {
      "service_worker": "background.js",
      "type": "module"
    },
    "minimum_chrome_version": "119"
  }